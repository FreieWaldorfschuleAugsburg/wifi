{
    "name": "waldorfaugsburg/wifi",
    "type": "project",
    "description": "Web-application for managing wifi access",
    "license": "MIT",
    "authors": [
        {
            "name": "Linus Groschke",
            "email": "contact@elektronisch.dev",
            "homepage": "https://elektronisch.dev"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4",
        "art-of-wifi/unifi-api-client": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
